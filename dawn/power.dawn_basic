define power
  native javascript
    var self = this
    self._out_number = null
    self._in_begin = function(pipe)
      self._value = new Dawn.bigRat();
    self._in_go = async function(pipe)
      if (pipe._out_number)
        pipe._out_number._call(self)
    self._in_number_base = function(pipe, number)
      self._value = number._value
    self._in_number_exponent = function(pipe, number)
      self._value = self._value.pow(number._value.round(true))
  endnative
