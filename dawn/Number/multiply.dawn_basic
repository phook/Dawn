define multiply
  native javascript
    this._out_number = null
    this._in_begin = function()
        if (!this.inputs_bound["_in_number_$"])
            this._bind_error("multiplication input not bound");       
    this._in_go = function()
      if (this._out_number)
        this._out_number(this)
    this._in_number_$ = function(number)
      this.data._value = this._value.multiply(number.data._value)
  endnative
