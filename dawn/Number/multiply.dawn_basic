define multiply
  native javascript
    this._out_number = null
    this._in_begin = function(pipe)
        if (!this.inputs_bound["_in_number_$"])
            this._bind_error("multiplication input not bound");       
    this._in_go = function(pipe)
      if (pipe._out_number)
        pipe._out_number(this)
    this._in_number_$ = function(pipe, number)
      this._value = this._value.multiply(number._value)
  endnative
