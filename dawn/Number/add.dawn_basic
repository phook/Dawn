define add
  native javascript
    this._out_number = new Dawn.bigRat();
    this._in_begin = function()
        if (!this.inputs_bound["_in_number_$"])
            this._bind_error("addition input not bound");       
    this._in_go = function()
      if (this._out_number)
        this._out_number(this);
    this._in_number_$ = function(number)
      this._value = this._value.add(number._value);
  endnative
