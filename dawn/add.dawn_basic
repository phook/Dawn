define add
  native javascript
    var self = this
    self._out_number = null
    self._in_begin = function(ref)
      ref._value = new Dawn.bigRat()
    self._in_go = async function(ref)
      if (ref._out_number)
        ref._out_number._call(ref)
    self._in_number_$ = function(ref, number)
      ref._value = ref._value.add(number._value)
  endnative
